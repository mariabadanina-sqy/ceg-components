{
  "name": "@client/design-system",
  "description": "Griddo client repo (styled components)",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "react",
    "styled-components",
    "griddo",
    "magma"
  ],
  "scripts": {
    "build:prebuild": "npm run build:themes && npm run build:icons",
    "build:themes": "node scripts/generateThemes",
    "build:icons": "node ./scripts/generateIcons.js",
    "start": "npm run build:prebuild && start-storybook -p 4000 -s ./static",
    "build:storybook": "npm run build:prebuild && NODE_ENV=production build-storybook -s ./static",
    "cp:editor": "nodemon --watch dist -x 'cp -r ./dist/* ~/ax/node_modules/components/dist'",
    "cp:api": "nodemon --watch dist -x 'cp -r ./dist/* ~/api/node_modules/components/dist && touch ~/api/__trigger__.js'",
    "cp:builder": "nodemon --watch dist -x 'cp -r ./dist/* ~/cx/node_modules/components/dist'",
    "rollup:watch": "npm run build:prebuild && BUILD_TYPE=patch npx rollup -cw",
    "dev": "concurrently \"npm:cp:editor\" \"npm:cp:api\" \"npm:cp:builder\" \"npm:rollup:watch\""
  },
  "dependencies": {
    "@griddo/core": "^0.9.4",
    "@sqymagma/elements": "^0.4.2",
    "@sqymagma/theme": "^0.3.7",
    "@svgr/rollup": "^4.3.0",
    "@svgr/webpack": "^4.3.0",
    "beautiful-react-hooks": "^0.22.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^4.2.2",
    "react-string-replace": "^0.4.4",
    "react-svg-loader": "^3.0.3",
    "ssr-window": "^2.0.0",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-alias": "^3.1.0",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "ajv": "^8.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "camelcase": "^6.2.0",
    "concurrently": "^6.2.0",
    "postcss": "^8.3.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "svgo": "^1.3.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
